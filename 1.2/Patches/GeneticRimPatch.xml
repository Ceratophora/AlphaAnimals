<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Genetic Rim</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>



		

		

			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[@Name = "GR_JawImplants"]/appliedOnFixedBodyParts</xpath>
				<value>
					<li>AA_InsectMouth</li>
					<li>AA_CrocodileJaw</li>
					<li>AA_SentientPlantMouth</li>
				</value>
			</li>



		
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[@Name = "GR_LegImplants"]/appliedOnFixedBodyParts</xpath>
				<value>
					<li>AA_SmallInsectLeg</li>

				</value>
			</li>







			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[@Name = "GR_TailImplants"]/appliedOnFixedBodyParts</xpath>
				<value>
					<li>AA_InsectTail</li>

				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "AA_MatureFleshbeast"]/race</xpath>
				<value>
					<useMeatFrom>GR_AberrantFleshbeast</useMeatFrom>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName = "GR_AberrantFleshbeast"]/comps</xpath>
				<value>
					<li Class="AlphaBehavioursAndEvents.CompProperties_Metamorphosis">
						<timeInYears>0.25</timeInYears>
						<pawnToTurnInto>AA_MatureFleshbeast</pawnToTurnInto>
					</li>
				</value>
			</li>


		</operations>
		</match>
	</Operation>


</Patch>

