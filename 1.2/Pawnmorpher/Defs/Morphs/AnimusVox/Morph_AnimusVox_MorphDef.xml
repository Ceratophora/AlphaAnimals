<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	
	
	
	<Pawnmorph.MorphDef>
		<defName>AA_AnimusMorph</defName>
		<label>animusmorph</label>
		<description>An Animusmorph is a canine / humanoid hybrid with latent psionic abilities. It will quickly bond with other colonists and reach with its mind to make happy everyone that treats it well.</description>
		<race>AA_AnimusVox</race>
		<classification>Fox</classification>
		<categories>
			<li>Arid</li>
			<li>Combat</li>
			<li>Social</li>
			<li>Canid</li>
		</categories>
		<fullTransformation>AA_PawnmorphAnimusVoxTF</fullTransformation>
		<partialTransformation>AA_PawnmorphAnimusVoxTFPartial</partialTransformation>
		<raceSettings>
			<!--<graphicsSettings>
				<skinColorOverride>(168,106,86)</skinColorOverride>
				<hairColorOverride>(0,0,0,0)</hairColorOverride>
			</graphicsSettings>-->
			
			<requiredMutations>
				<li>AA_AnimusVoxSnout</li>
				<li>AA_AnimusVoxEar</li>
			</requiredMutations>
		</raceSettings>
		<transformSettings>
			<transformationMemory>FoxTransformationMemory</transformationMemory>
			<revertedMemory>FoxRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>