    <stuffProps>
      <color>(108,157,177)</color>
      <commonality>0.001</commonality>
      <statFactors>
        <MaxHitPoints>1.5</MaxHitPoints>
      </statFactors>
    </stuffProps>
  </ThingDef>
  
