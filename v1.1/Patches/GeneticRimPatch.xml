<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			








			<li Class="PatchOperationFindMod">
				<mods>
					<li>Genetic Rim</li>
				</mods> 
				<match Class="PatchOperationAdd"> 
					<xpath>/Defs/RecipeDef[@Name = "GR_JawImplants"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>AA_InsectMouth</li>
						<li>AA_CrocodileJaw</li>
						<li>AA_SentientPlantMouth</li>
					</value>
				</match>
			</li>




			<li Class="PatchOperationFindMod">
				<mods>
					<li>Genetic Rim</li>
				</mods> 
				<match Class="PatchOperationAdd"> 
					<xpath>/Defs/RecipeDef[@Name = "GR_LegImplants"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>AA_SmallInsectLeg</li>

					</value>
				</match>
			</li>








			<li Class="PatchOperationFindMod">
				<mods>
					<li>Genetic Rim</li>
				</mods> 
				<match Class="PatchOperationAdd"> 
					<xpath>/Defs/RecipeDef[@Name = "GR_TailImplants"]/appliedOnFixedBodyParts</xpath>
					<value>
						<li>AA_InsectTail</li>

					</value>
				</match>
			</li>



		</operations>
	</Operation>


</Patch>

